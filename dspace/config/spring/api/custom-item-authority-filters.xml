<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

    <bean id="projectAuthorityFilter" class="org.dspace.content.authority.ProjectAuthorityFilter">
        <property name="supportedEntities">
            <list>
				<value>impactpathwaystep</value>
				<value>impactpathway</value>
				<value>application</value>
				<value>award</value>
				<value>contribution_fundingprogramme</value>
				<value>cooperationpartner</value>
				<value>ethic</value>
				<value>Event</value>
				<value>Funding</value>
				<value>pot_societalimpact</value>
				<value>condition</value>
				<value>gender</value>
				<value>innovationidea</value>
				<value>milestone</value>
				<value>step</value>
				<value>physical_object</value>
				<value>other</value>
				<value>Patent</value>
				<value>plan_publication</value>
				<value>process_event</value>
				<value>proj_objective</value>
				<value>projectpartner</value>
				<value>parentproject</value>
				<value>Publication</value>
				<value>Product</value>
				<value>research_result</value>
				<value>innovationpotential</value>
				<value>spinoff</value>
				<value>stateofart</value>
				<value>subcontractor</value>
				<value>targetgroup</value>
				<value>task</value>
				<value>team</value>
				<value>unexpected_result</value>
				<value>workpackage</value>
				<value>exploitationplanstep</value>
				<value>exploitationplan</value>
				<value>Project</value>
            </list>
        </property>
    </bean>

    <bean id="fundingObjectiveAuthorityFilter" class="org.dspace.content.authority.FundingObjectiveAuthorityFilter">
        <property name="supportedEntities">
            <list>
            	<value>fundingobjective</value>
            </list>
        </property>
    </bean>
    
</beans>