<?xml version="1.0" encoding="UTF-8"?>
<beans
    xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="capture-website-token" class="org.dspace.app.capture.mapper.PropertiesHeaderValueMapper">
        <constructor-arg name="propertyName" value="capture-website.jwt.token"/>
        <constructor-arg name="defaultValue"><null/></constructor-arg>
        <constructor-arg name="prefix" value="Bearer"/>
    </bean>
    
   <bean id="context-user-mapper" class="org.dspace.app.capture.mapper.ContextUserHeaderValueMapper"/>
    
    <bean id="capture-website-headers" class="java.util.HashMap">
        <constructor-arg>
            <map>
                <entry 
                    key="Authorization"
                    value-ref="capture-website-token"
                />
                <entry
                    key="X-On-Behalf-Of"
                    value-ref="context-user-mapper"
                />
            </map>
        </constructor-arg>
    </bean>
    
    <bean id="uuid-extractor" class="org.dspace.app.versioning.action.capture.mapper.ItemUuidExtractor"/>
    
    <bean id="simple-provider" class="org.dspace.app.versioning.model.SimpleItemProvider"/>

    <bean id="graphtargetgroupnace-provider" class="org.dspace.app.versioning.model.FacetDiscoveryConfigurationItemProvider">
        <constructor-arg name="discoveryConfiguration" value="RELATION.Project.targetgroup"/>
        <constructor-arg name="facetName" value="graphtargetgroupnace"/>
    </bean>

    <bean id="graphtargetgroupenterprise-provider" class="org.dspace.app.versioning.model.FacetDiscoveryConfigurationItemProvider">
        <constructor-arg name="discoveryConfiguration" value="RELATION.Project.targetgroup"/>
        <constructor-arg name="facetName" value="graphtargetgroupenterprise"/>
    </bean>

    <bean id="graphtargetgrouppolitical-provider" class="org.dspace.app.versioning.model.FacetDiscoveryConfigurationItemProvider">
        <constructor-arg name="discoveryConfiguration" value="RELATION.Project.targetgroup"/>
        <constructor-arg name="facetName" value="graphtargetgrouppolitical"/>
    </bean>

    <bean id="graphtargetgroupurbanrural-provider" class="org.dspace.app.versioning.model.FacetDiscoveryConfigurationItemProvider">
        <constructor-arg name="discoveryConfiguration" value="RELATION.Project.targetgroup"/>
        <constructor-arg name="facetName" value="graphtargetgroupurbanrural"/>
    </bean>

    <bean id="graphcoopparttype-provider" class="org.dspace.app.versioning.model.FacetDiscoveryConfigurationItemProvider">
        <constructor-arg name="discoveryConfiguration" value="RELATION.Project.cooperationpartner"/>
        <constructor-arg name="facetName" value="graphcoopparttype"/>
    </bean>

    <bean id="graphcooppartnace-provider" class="org.dspace.app.versioning.model.FacetDiscoveryConfigurationItemProvider">
        <constructor-arg name="discoveryConfiguration" value="RELATION.Project.cooperationpartner"/>
        <constructor-arg name="facetName" value="graphcooppartnace"/>
    </bean>

    <bean id="graphcooppartdestatis-provider" class="org.dspace.app.versioning.model.FacetDiscoveryConfigurationItemProvider">
        <constructor-arg name="discoveryConfiguration" value="RELATION.Project.cooperationpartner"/>
        <constructor-arg name="facetName" value="graphcooppartdestatis"/>
    </bean>

    <bean id="graphcooppartaddressCountry-provider" class="org.dspace.app.versioning.model.FacetDiscoveryConfigurationItemProvider">
        <constructor-arg name="discoveryConfiguration" value="RELATION.Project.cooperationpartner"/>
        <constructor-arg name="facetName" value="graphcooppartaddressCountry"/>
    </bean>
    
    <!-- IMPACTPATHWAY ACTION CONFIGURATION -->
    
    <bean id="impactpathway-capturable" class="org.dspace.app.capture.model.CapturableScreenConfiguration">
        <constructor-arg name="element" value="#ipw"/>
        <constructor-arg name="removeElements"><null/></constructor-arg>
        <constructor-arg name="style"><null/></constructor-arg>
        <constructor-arg name="type" value="jpeg"/>
        <constructor-arg name="scale" value="1"/>
        <constructor-arg name="waitForElement"><null/></constructor-arg>
        <constructor-arg name="delay" value="3"/>
        <constructor-arg name="timeout" value="30"/>
        <constructor-arg name="headersConfiguration" ref="capture-website-headers"/>
    </bean>
    
    <bean id="impactpathway-discovery" class="org.dspace.app.versioning.model.DiscoveryRelationItemProvider">
        <!-- RELATION.Project.impactpathway -->
        <constructor-arg name="relationName" value="impactpathway"/>
    </bean>
    
    <bean id="impactpathway-url" class="org.dspace.app.versioning.action.capture.mapper.SimplePrintableViewItemUrlMapper">
        <constructor-arg name="baseUrl" value="entities/impactpathway"/>
    </bean>
    
    <bean id="IMPACTPATHWAY" class="org.dspace.app.versioning.action.capture.CaptureScreenActionConfiguration">
        <constructor-arg name="configuration" ref="impactpathway-capturable"/>
        <constructor-arg name="bundleName" value="IMPACTPATHWAY"/>
        <constructor-arg name="itemProvider" ref="impactpathway-discovery"/>
        <constructor-arg name="itemUrlMapper" ref="impactpathway-url"/>
    </bean>

    <bean id="CLEAR-IMPACTPATHWAY" class="org.dspace.app.versioning.action.clear.bundle.ClearBundleActionConfiguration">
        <constructor-arg name="configuration" value="IMPACTPATHWAY"/>
    </bean>
    
    <bean id="IMPACTPATHWAY-CONFIGURATIONS" class="java.util.ArrayList">
        <constructor-arg>
            <list>
                <ref bean="IMPACTPATHWAY" />
            </list>
        </constructor-arg>
    </bean>

    <bean id="COMPOSITE-IMPACTPATHWAY" class="org.dspace.app.versioning.action.group.CompositeVersioningActionConfiguration">
        <constructor-arg name="configuration" ref="CLEAR-IMPACTPATHWAY"/>
        <constructor-arg name="configurations" ref="IMPACTPATHWAY-CONFIGURATIONS"/>
    </bean>

    <!-- IMPACTPATHWAY ACTION CONFIGURATION -->
    
    <!-- WORKINGPLAN ACTION CONFIGURATION -->
    
    <bean id="workingplan-authority" class="org.dspace.app.versioning.model.MetadataAuthorityItemProvider">
        <constructor-arg name="metadataField" value="synsicris.relation.workingplan"/>
    </bean>
    
    <bean id="workingplan-base-url" class="org.dspace.app.versioning.action.capture.mapper.AuthorityRelatedItemUrlMapper">
        <constructor-arg name="baseUrl" value="entities/project"/>
        <constructor-arg name="relationMetadata" value="synsicris.relation.project"/>
        <constructor-arg name="suffix" value="workingplan"/>
    </bean>

    <bean id="workingplan-url" class="org.dspace.app.versioning.action.capture.mapper.SimpleParametersItemUrlMapper">
        <constructor-arg name="itemUrlMapper" ref="workingplan-base-url"/>
        <constructor-arg name="parameters">
            <bean id="print-view-params" class="java.util.HashMap">
		        <constructor-arg>
		            <map>
		                <entry 
		                    key="view"
		                    value="print"
		                />
		            </map>
		        </constructor-arg>
		    </bean>
        </constructor-arg>
    </bean>
    
    <bean id="workingplan-capturable" class="org.dspace.app.capture.model.CapturableScreenConfiguration">
        <constructor-arg name="element" value=".sidenav-container"/>
        <constructor-arg name="removeElements"><null/></constructor-arg>
        <constructor-arg name="style"><null/></constructor-arg>
        <constructor-arg name="type" value="jpeg"/>
        <constructor-arg name="scale" value="1"/>
        <constructor-arg name="waitForElement"><null/></constructor-arg>
        <constructor-arg name="delay" value="10"/>
        <constructor-arg name="timeout" value="30"/>
        <constructor-arg name="headersConfiguration" ref="capture-website-headers"/>
    </bean>
    
    <bean id="WORKINGPLAN" class="org.dspace.app.versioning.action.capture.CaptureScreenActionConfiguration">
        <constructor-arg name="configuration" ref="workingplan-capturable"/>
        <constructor-arg name="bundleName" value="WORKINGPLAN"/>
        <constructor-arg name="itemProvider" ref="workingplan-authority"/>
        <constructor-arg name="itemUrlMapper" ref="workingplan-url"/>
    </bean>
    
    <bean id="CLEAR-WORKINGPLAN" class="org.dspace.app.versioning.action.clear.bundle.ClearBundleActionConfiguration">
        <constructor-arg name="configuration" value="WORKINGPLAN"/>
    </bean>
    
    <bean id="WORKINGPLAN-CONFIGURATIONS" class="java.util.ArrayList">
        <constructor-arg>
            <list>
                <ref bean="WORKINGPLAN" />
            </list>
        </constructor-arg>
    </bean>
    
    <bean id="COMPOSITE-WORKINGPLAN" class="org.dspace.app.versioning.action.group.CompositeVersioningActionConfiguration">
        <constructor-arg name="configuration" ref="CLEAR-WORKINGPLAN"/>
        <constructor-arg name="configurations" ref="WORKINGPLAN-CONFIGURATIONS"/>
    </bean>
    
    <!-- WORKINGPLAN ACTION CONFIGURATION -->

    <!-- GRAPH ACTION CONFIGURATION -->
    
    <bean id="graph-capturable" class="org.dspace.app.capture.model.CapturableScreenConfiguration">
        <constructor-arg name="element"><null/></constructor-arg>
        <constructor-arg name="removeElements"><null/></constructor-arg>
        <constructor-arg name="style"><null/></constructor-arg>
        <constructor-arg name="type" value="jpeg"/>
        <constructor-arg name="scale" value="1"/>
        <constructor-arg name="waitForElement" value="ds-graph"/>
        <constructor-arg name="delay" value="3"/>
        <constructor-arg name="timeout" value="30"/>
        <constructor-arg name="headersConfiguration" ref="capture-website-headers"/>
    </bean>
    
    <bean id="graph-authority" class="org.dspace.app.versioning.model.MetadataAuthorityItemProvider">
        <constructor-arg name="metadataField" value="synsicris.relation.workingplan"/>
    </bean>
    
    <bean id="graph-itemType-params" class="java.util.HashMap">
        <constructor-arg>
            <map>
                <entry 
                    key="filterName"
                    value="graphitemtype"
                />
                <entry 
                    key="configuration"
                    value="defaultConfiguration"
                />
            </map>
        </constructor-arg>
    </bean>

    <bean id="graph-page-url" class="org.dspace.app.versioning.action.capture.mapper.StaticItemUrlMapper">
        <constructor-arg name="baseUrl" value="graph-page"/>
    </bean>
    
    <bean id="graph-scope-params" class="java.util.HashMap">
        <constructor-arg>
            <map>
                <entry 
                    key="scope"
                    value-ref="uuid-extractor"
                />
            </map>
        </constructor-arg>
    </bean>
    
    <bean id="graph-url" class="org.dspace.app.versioning.action.capture.mapper.GraphRelatedItemUrlMapper">
        <constructor-arg name="parameters" ref="graph-itemType-params"/>
        <constructor-arg name="itemUrlMapper" ref="graph-page-url"/>
        <constructor-arg name="dynamicParameters" ref="graph-scope-params"/>
    </bean>

    <bean id="GRAPH" class="org.dspace.app.versioning.action.capture.CaptureScreenActionConfiguration">
        <constructor-arg name="configuration" ref="graph-capturable"/>
        <constructor-arg name="bundleName" value="GRAPH"/>
        <constructor-arg name="itemProvider" ref="simple-provider"/>
        <constructor-arg name="itemUrlMapper" ref="graph-url"/>
    </bean>
    
